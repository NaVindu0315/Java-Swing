package DRGDROP;

import javax.swing.*;

public class FrameClass extends JFrame {

    DragClass dragClass = new DragClass();

    public FrameClass(){
        this.add(dragClass);
       // this.add(dragClass);
        this.setTitle("Drag And Drop demno");
        this.setSize(600,600);
        this.setDefaultCloseOperation(EXIT_ON_CLOSE);
        this.setVisible(true);
      //  this.setLayout(null);


    }

}
