package Animations;

import javax.swing.*;

public class AFrame extends JFrame {

    Apanel panel;

    public AFrame(){
        panel = new Apanel();



        this.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
        this.add(panel);
        this.pack();
        this.setLocationRelativeTo(null);
        this.setVisible(true);
    }
}

