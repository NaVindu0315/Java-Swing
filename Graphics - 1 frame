package Graphics;

import javax.swing.*;
import java.awt.*;

public class Gframe extends JFrame {

    Gpanel panel;

    public Gframe(){

        panel = new Gpanel();



        this.setDefaultCloseOperation(EXIT_ON_CLOSE);
        //this.setSize(500,500);
        this.add(panel);
        this.pack();
        this.setLocationRelativeTo(null);
        this.setVisible(true);
    }


}
